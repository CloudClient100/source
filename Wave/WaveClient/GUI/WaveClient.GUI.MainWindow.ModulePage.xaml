                                    </TransformGroup>
                                </TabItem.RenderTransform>
                                <Border Background="White" BorderBrush="Blue" BorderThickness="5">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="31*"/>
